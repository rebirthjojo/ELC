<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.member.mybatis.SearchMapper">

    <select id="findSearchByKeyword" resultType="com.example.member.dto.SearchDTO">
        SELECT
            uid,
            lecture_name,
            tutor_name
        FROM
            course_tb
        WHERE
            course_name LIKE CONCAT('%', #{query}, '%')
        LIMIT 10
    </select>
</mapper>